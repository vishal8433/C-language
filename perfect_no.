//Compile on- dev c++
//Program name - perfect no.





#include<stdio.h>
int main()
{
	int n,i=1,s=0;
	printf("enter a no.\n");
	scanf("%d",&n);
	while(i<n-1)
	{
		if(n%i==0)
		{
			s=s+i;
		}
		i++;
	}
	if(s==n)
	printf("perfect no.");
	else 
	printf("not perfect no.");
	return 0;
}
