//Program name - find the sum of n natural number




#include<stdio.h>
int main()
{
	int i=1,n,sum=0;
	printf("welcome to this program\n");
	printf("enter the value of n \n");
	scanf("%d",&n);
	while(i<=n)
	{
		sum=sum+i;
		i++;
	}
	printf("your sum is %d",sum);
	return 0;
}
