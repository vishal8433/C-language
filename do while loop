//Program name - do while loop




#include<stdio.h>

int main()
{
    printf("Studytonight - Best place to learn\n");
    int i = 10; 

    do 
    {
        printf("i = %d\n",i);
        i = i-1;   
    }
    while(i > 0);
    
    printf("\n\The value of i after exiting the loop is %d\n\n", i);
    printf("\n\n\t\t\tCoding is Fun !\n\n\n");
    return 0;
}
