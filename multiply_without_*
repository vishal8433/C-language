//Program name - multiply two number without *




#include<stdio.h>
int main()
{
	int x,y,p=0,i=1;
	printf("enter the value of x and y (x*y)=  \n");
	scanf("%d%d",&x,&y);
	while(i<=y)
	{
		p=p+x;
		i++;
		
	}
	printf("multiplication of  2 no. (%d*%d)=%d",x,y,p);
	return 0;
}
